                                        .input-w-select
                                            .col-xs-12.col-sm-6
                                                .form-group.common-form-group
                                                    .input-wrapper.input-wrapper-fluid
                                                        input(type="text" placeholder="Стоимость").padded

                                            .col-xs-12.col-sm-6
                                                .form-group.common-form-group
                                                    .dropdown.box-dropdown
                                                        a.box-dropdown__text#dLabel(data-target='#', href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                                                            | Валюта: RUB
                                                            span.caret
                                                        ul.box-dropdown__list.dropdown-menu(aria-labelledby='dLabel')
                                                            li: a(href="#") item 1
                                                            li: a(href="#") item 2

                                        .col-xs-12
                                            .form-group.common-form-group
                                                .input-wrapper.input-wrapper-fluid
                                                    input(type="text" placeholder="Описание товара").padded

                                        .col-xs-12.col-sm-4
                                            .form-group.common-form-group.mb-xs-0
                                                label Пол

                                        .col-xs-12.col-sm-8
                                            .form-group.common-form-group
                                                .row
                                                    .col-xs-4.usual-radio-block
                                                        input(type="radio" value="female" name="sex" checked id="all")
                                                        label(for="all") Все
                                                    .col-xs-4.usual-radio-block
                                                        input(type="radio" value="male" name="sex" id="male")
                                                        label(for="male") Мальчик
                                                    .col-xs-4.usual-radio-block
                                                        input(type="radio" value="male" name="sex" id="male")
                                                        label(for="male") Девочка

                                        .col-xs-12.col-sm-4
                                            .form-group.common-form-group.mb-xs-0
                                                label Возраст

                                        .col-xs-12.col-sm-8
                                            .form-group.common-form-group
                                                .row
                                                    .col-xs-4.usual-radio-block
                                                        input(type="radio" value="female" name="age" checked id="fr")
                                                        label(for="fr") 0-12 мес.
                                                    .col-xs-4.usual-radio-block
                                                        input(type="radio" value="male" name="age" id="sd")
                                                        label(for="sd") 1-3 года
                                                    .col-xs-4.usual-radio-block
                                                        input(type="radio" value="male" name="age" id="th")
                                                        label(for="th") от 3 лет

                                        .col-xs-12
                                            .box-row
                                                .box-square-button.with-image
