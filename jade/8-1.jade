                        .col-md-12
                             .box
                                .box-head
                                    .text контактные данные
                                .box-content
                                    .row
                                        .col-xs-12.col-sm-6
                                            .form-group.common-form-group
                                                .input-wrapper.input-wrapper-fluid
                                                    input(type="text" placeholder="Название").padded

                                        .col-xs-12.col-sm-6
                                            .form-group.common-form-group
                                                .input-wrapper.input-wrapper-fluid
