extends ./index.jade

block content
    .subhead.clearfix
        .pull-left
            .filter-name(data-toggle="modal" data-target="#modalFleaMarket") Фильтр
        ul(class='nav nav-tabs' role='tablist')
            li(role="presentation" class="active")
                a(href='#home' aria-controls='home' role='tab' data-toggle='tab') Все
            li(role="presentation")
                a(href='#profile' aria-controls='profile' role='tab' data-toggle='tab') Избранные

            li(role="presentation")
                a(href='#my-items' aria-controls='profile' role='tab' data-toggle='tab') Мои товары

            li(role="presentation")
                a(href='#sail-item' aria-controls='profile' role='tab' data-toggle='tab') Продать товар

    .content-wrapper
        .row
            .tab-content
                div(role="tabpanel" class="tab-pane active" id="home")
                    .col-md-12
                        .box.gray.no-shadow
                            .box-content.no-padding
                                .fleaMarket.clearfix
                                    .col-sm-6.col-xs-12.fleaMarket-block
                                        .fleaMarket-block--head
                                            .text Футболка с рисунком. Размер: 74

                                        .fleaMarket-block--body
                                            .item-slider
                                                div(id="carousel-example-generic" class="carousel slide" data-ride="carousel")
                                                    div(class="carousel-inner" role="listbox")
                                                        .item.active
                                                            .item-image-wrap
                                                                .item-image-content
                                                                    img(src="/img/item-image.png", alt="")
                                                        .item
                                                            .item-image-wrap
                                                                .item-image-content
                                                                    img(src="/img/item-image.png", alt="")

                                                    a(class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev")
                                                        span(class="glyphicon glyphicon-chevron-left" aria-hidden="true")
                                                        span.sr-only Previous
                                                    a(class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next")
                                                        span(class="glyphicon glyphicon-chevron-right" aria-hidden="true")
                                                        span.sr-only Next

                                    .col-sm-6.col-xs-12.fleaMarket-block
                                        .fleaMarket-block--head
                                            .text Цена: 100 грн.
                                            .like-block

                                        .fleaMarket-block--body
                                            .sail-block
                                                .av-block
                                                    img(src="img/avatar-ex.png", alt="")
                                                img(src="img/mom-icon.png" width="16" class="mom-icon-block" height="42")
                                                .name Марина Кравец

                                            .write-block
                                                button.pink-button Написать продавцу
                                            .item-text
                                                p Здесь будет текст описания товара, состав, особенности. Здесь будет текст описания товара, состав, особенности.
